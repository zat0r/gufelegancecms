<%- include('Layouts/Header'); -%>
<section class="content">
    <div class="container-fluid">
        <div class="block-header">
            <h2 data-toggle="tooltip" data-placement="top" title="" data-original-title="Tooltip on left"><%= title %></h2>
        </div>
        <div class="row clearfix">
                <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
                    <div class="info-box bg-orange hover-expand-effect">
                        <div class="icon">
                                <i class="fas fa-user-plus"></i>
                        </div>
                        <div class="content">
                            <div class="text">تسجيل جديد</div>
                            <div class="number count-to" data-from="0" data-to="31" data-speed="1000" data-fresh-interval="20">31</div>
                        </div>
                    </div>
                </div>
                <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
                        <div class="info-box bg-blue hover-expand-effect">
                            <div class="icon"><i class="far fa-gem"></i></div>
                            <div class="content">
                                <div class="text">قريبا</div>
                                <div class="number count-to" data-from="0" data-to="0" data-speed="1000" data-fresh-interval="20">0</div>
                            </div>
                        </div>
                    </div>
            </div>
    </div>
</section>
<%- include('Layouts/Footer'); -%>

