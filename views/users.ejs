<%- include('Layout/header'); -%>
<main role="main" class="col-md-9 ml-sm-auto col-lg-10 px-4">
    <div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">
            <h1 class="h2">قائمة العملاء</h1>
            <div class="btn-toolbar mb-2 mb-md-0">
              <div class="btn-group mr-2">
                <button type="button" class="btn btn-sm btn-outline-secondary" onclick="OpenAddNew()">إضافة</button>
                <button type="button" onclick="PageData()" class="btn btn-sm btn-outline-secondary">تحميل</button>
              </div>
              <button type="button" class="btn btn-sm btn-outline-secondary dropdown-toggle"><i class="fas fa-calendar-week"></i>أخر أسبوع</button>
            </div>
          </div>
          <section class="container">
          <div class="row">
            <div class="col-lg-12 card">
                <div class="card-header"><h3>العملاء المضافين</h3></div>
                <div class="card-body">
                      <table id="example" class="table table-striped table-bordered" style="width:100%">
                              <thead>
                                      <tr>
                                          <th>الإسم</th>
                                          <th>الجوال</th>
                                          <th>المدينة</th>
                                          <th>العنوان</th>
                                          <th>إجرائات</th>
                                      </tr>
                                  </thead>
                                  <tbody id="Data">
                                  </tbody>
                      </table>
                </div>
            </div>
          </div>
          </section>
    </main>
